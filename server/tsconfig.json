{
	"compilerOptions": {
		"target": "ESNext",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"module": "NodeNext",
		"rootDir": "./src",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"strictPropertyInitialization": false
	}
}